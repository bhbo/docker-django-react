{
  "name": "docker-boilerplate",
  "version": "1.0.0",
  "description": "Docker Boilerplate Project",
  "scripts": {
    "serve": "babel-node webpack/server.js",
    "build": "babel-node ./node_modules/webpack/bin/webpack --config webpack/config.prod.js --display-error-details",
    "develop": "npm install -q && npm run serve",
    "test": "mocha --opts .mocha.opts",
    "testall": "npm test src/**/*.test.js",
    "testwatch": "npm test --reporter min --watch src/**/*.test.js",
    "testcover": "istanbul cover _mocha -- --opts mocha.opts src/**/*.test.js",
    "lint": "eslint -c .eslintrc src || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elielagmay/docker-boilerplate"
  },
  "author": "Elie Lagmay <jmlagmay@gmail.com>",
  "private": true,
  "engines": {
    "node": "^6.3.0",
    "npm": "^3.0.0"
  },
  "dependencies": {
    "assets-webpack-plugin": "3.4.0",
    "babel-cli": "^6.0.0",
    "babel-core": "6.9.1",
    "babel-eslint": "6.0.5",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "enzyme": "2.3.0",
    "eslint": "2.13.1",
    "eslint-plugin-react": "5.2.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.21.0",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.3.0",
    "json-loader": "0.5.4",
    "mocha": "2.5.3",
    "moment-timezone": "0.5.5",
    "poststylus": "0.2.3",
    "react": "15.1.0",
    "react-addons-test-utils": "15.1.0",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "rucksack-css": "0.8.6",
    "striptags": "2.1.1",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.1.1",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "1.14.1",
    "whatwg-fetch": "1.0.0"
  }
}
